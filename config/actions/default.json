{
	"extract":{
		"executable":"/usr/bin/unrar",
		"arguments":"-o- x \"%%torrentpath%%/*.rar\""
	},
	"ping":{
		"executable":"ping",
		"arguments":"-t 127.0.0.1"
	},
	"move":{
		"executable":"cp",
		"arguments":"-r %%torrentpath%% /new/path/folder"
	},	
	"none":{
		"executable":"",
		"arguments":""
	}	
}
